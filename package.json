{
  "name": "react-ie8-boilerplate",
  "version": "0.0.1",
  "description": "React-Webpack-hot reload boilerplate project with partial support for IE8",
  "main": "src/main.jsx",
  "scripts": {
    "build": "webpack -p",
    "clean": "rm -fr dist/*",
    "preprod:start": "npm run clean & npm run build",
    "prod:start": "ws -r '/static/* -> /dist/$1' ",
    "start": "node webpack-dev-server.js"
  },
  "author": "Chris Kobrzak <chris.kobrzak@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.5.0",
    "babel-plugin-transform-es3-property-literals": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "local-web-server": "^1.2.1",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.8.4"
  }
}
